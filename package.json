  "devDependencies": {
    "copy-webpack-plugin": "^9.0.1",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.1"
  }
}
